{
  "name": "flarum-cli",
  "description": "A CLI for developing Flarum extensions",
  "version": "0.0.0",
  "author": "Flarum Team",
  "bin": {
    "flarum-cli": "./bin/run"
  },
  "bugs": "https://github.com/flarum/flarum-cli/issues",
  "devDependencies": {
    "@oclif/dev-cli": "^1.26.0",
    "@oclif/test": "^1.2.8",
    "@types/chai": "^4.2.15",
    "@types/chalk": "^2.2.0",
    "@types/mem-fs": "^1.1.2",
    "@types/mem-fs-editor": "^7.0.0",
    "@types/mocha": "^5.2.7",
    "@types/node": "^10.17.55",
    "@types/prompts": "^2.0.9",
    "@types/yosay": "^0.0.29",
    "chai": "^4.3.3",
    "eslint": "^5.16.0",
    "eslint-config-oclif": "^3.1.0",
    "eslint-config-oclif-typescript": "^0.1.0",
    "globby": "^10.0.2",
    "mocha": "^8.3.1",
    "nyc": "^14.1.1",
    "prettier": "^2.2.1",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.9"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/src",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/flarum/flarum-cli",
  "keywords": [
    "oclif"
  ],
  "license": "MIT",
  "main": "src/index.js",
  "oclif": {
    "commands": "./src/commands",
    "bin": "flarum-cli",
    "helpClass": "./src/help",
    "plugins": [
      "@oclif/plugin-help",
      "@tiagonapoli/oclif-plugin-spaced-commands"
    ],
    "topics": {
      "generate:backend": {
        "description": "add backend boilerplate code to an existing Flarum extension"
      },
      "generate": {
        "description": "add boilerplate code to an existing Flarum extension"
      },
      "infra": {
        "description": "add/modify infrastructure for an existing Flarum extension"
      }
    }
  },
  "repository": "flarum/flarum-cli",
  "scripts": {
    "format": "prettier --write src",
    "postpack": "rm -f oclif.manifest.json",
    "posttest": "eslint . --ext .ts --config .eslintrc",
    "prepack": "rm -rf src && tsc -b && oclif-dev manifest && oclif-dev readme",
    "test": "nyc --extension .ts mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif-dev readme && git add README.md"
  },
  "types": "src/index.d.ts",
  "dependencies": {
    "@oclif/command": "^1.8.0",
    "@oclif/config": "^1.17.0",
    "@oclif/plugin-autocomplete": "^0.3.0",
    "@oclif/plugin-help": "^3.2.2",
    "@tiagonapoli/oclif-plugin-spaced-commands": "^0.0.6",
    "chalk": "^2.4.2",
    "mem-fs": "^1.2.0",
    "mem-fs-editor": "^8.0.0",
    "prompts": "^2.4.0",
    "spdx-license-list": "^6.4.0",
    "tslib": "^1.14.1",
    "yosay": "^2.0.2"
  }
}
