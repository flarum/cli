{
  "rules": {
    "unicorn/no-abusive-eslint-disable": "off",
    "@typescript-eslint/ban-ts-ignore": "off",
    "indent": ["error", 2, {"MemberExpression": 1}],
    "valid-jsdoc": "off",
    "semi": [2, "always"],
    "max-params": ["error", 6],
    "object-curly-spacing": "off"
  },
  "extends": [
    "oclif",
    "oclif-typescript"
  ]
}
